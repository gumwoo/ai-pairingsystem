<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 400 300">
  <!-- Background -->
  <rect width="400" height="300" fill="#f5f5f5" />
  
  <!-- Rum bottle -->
  <rect x="150" y="50" width="100" height="200" rx="10" ry="10" fill="#4E342E" />
  <rect x="160" y="60" width="80" height="180" rx="5" ry="5" fill="#5D4037" />
  
  <!-- Bottle neck -->
  <rect x="180" y="30" width="40" height="30" rx="5" ry="5" fill="#3E2723" />
  <rect x="185" y="35" width="30" height="20" rx="2" ry="2" fill="#4E342E" />
  
  <!-- Bottle cap -->
  <rect x="180" y="20" width="40" height="10" rx="2" ry="2" fill="#FBC02D" />
  
  <!-- Label -->
  <rect x="170" y="120" width="60" height="80" rx="5" ry="5" fill="#FFECB3" />
  <text x="200" y="150" font-family="Arial" font-size="12" text-anchor="middle" fill="#4E342E">AGED</text>
  <text x="200" y="170" font-family="Arial" font-size="14" text-anchor="middle" fill="#4E342E">RUM</text>
  <text x="200" y="190" font-family="Arial" font-size="8" text-anchor="middle" fill="#4E342E">40% ALC/VOL</text>
  
  <!-- Palm tree decoration -->
  <path d="M185,100 L190,80 L195,100 M190,80 L185,90 M190,80 L195,90" fill="none" stroke="#FBC02D" stroke-width="1.5" />
  <path d="M210,100 L215,80 L220,100 M215,80 L210,90 M215,80 L220,90" fill="none" stroke="#FBC02D" stroke-width="1.5" />
  
  <!-- Glass with rum -->
  <path d="M280,230 L300,150 L320,150 L340,230 Z" fill="#6D4C41" />
  <path d="M280,230 L340,230 L340,240 L280,240 Z" fill="#6D4C41" />
  <ellipse cx="310" cy="150" rx="15" ry="5" fill="#6D4C41" />
  <ellipse cx="310" cy="240" rx="30" ry="10" fill="#6D4C41" />
  <path d="M280,230 L300,150 L320,150 L340,230 Z" fill="none" stroke="#5D4037" stroke-width="2" />
  <ellipse cx="310" cy="150" rx="15" ry="5" fill="none" stroke="#5D4037" stroke-width="2" />
  <ellipse cx="310" cy="240" rx="30" ry="10" fill="none" stroke="#5D4037" stroke-width="2" />
  
  <!-- Ice cubes -->
  <rect x="295" y="170" width="15" height="15" rx="2" ry="2" fill="#BBDEFB" opacity="0.7" />
  <rect x="310" y="190" width="15" height="15" rx="2" ry="2" fill="#BBDEFB" opacity="0.7" />
</svg>